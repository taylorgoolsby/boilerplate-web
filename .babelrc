{
  "env": {
    "production": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": false,
            "targets": {
              "browsers": "last 2 chrome versions"
            }
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": [
        "styled-components"
      ]
    },
    "development": {
      "presets": [
        [
          "env",
          {
            "loose": true,
            "modules": false,
            "targets": {
              "browsers": "last 2 chrome versions"
            }
          }
        ],
        "react",
        "stage-2"
      ],
      "plugins": [
        "react-hot-loader/babel",
        "styled-components"
      ]
    }
  }
}